// Place your key bindings in this file to override the defaults
[
  {
    "key": "cmd+k",
    "command": "workbench.action.terminal.clear",
    "when": "terminalFocus"
  },
  {
    "key": "shift+cmd+i",
    "command": "runCommands",
    "args": {
      "commands": [
        "workbench.action.chat.newChat",
        "workbench.action.chat.openAgent"
      ]
    }
  }
]
